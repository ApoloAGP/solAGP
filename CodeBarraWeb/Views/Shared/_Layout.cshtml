<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A preview of the jQuery UI Bootstrap theme">
    <meta name="author" content="Addy Osmani">

    <!-- Le styles -->
    @Styles.Render("~/bootstrap/css")
    @Styles.Render("~/Bootstrap/css/custom-theme")
    @Styles.Render("~/Bootstrap/assets/css")
    @Styles.Render("~/Bootstrap/assets/js/google-code-prettify")
</head>
<body data-spy="scroll" data-target=".bs-docs-sidebar" data-twttr-rendered="true">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="brand">CodeBarraWeb</a>
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li class="active">
                            <a href="@Url.Action("Index", "Reporte")">Reporte</a>
                        </li>
                        <li>
                            <a href="@Url.Action("ReporteTV", "Reporte")">ReporteTV</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <section class="container">
            @RenderBody()
        </section>
    </div>

            @RenderSection("menu", required: false)
            @Scripts.Render("~/Bootstrap/assets/js")
            @Scripts.Render("~/Bootstrap/assets/js/google-code-prettify")
            @Scripts.Render("~/Bootstrap/assets/js2")
            @RenderSection("scripts", required: false)

</body>
</html>
